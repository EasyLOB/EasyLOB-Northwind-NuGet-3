@model EmployeeCollectionModel

@{
    string CSHTML = "Employee - Index.cshtml";
}

<div id="ZAjax"></div>

<script>
    $(function () {
        try {
            zUrlDictionaryClear();

            $("#ZAjax").load("@(Url.Action("Search", "Employee", null, Request.Url.Scheme))", function(responseText, textStatus, jqXHR) { zAjaxLoadComplete(responseText, textStatus, jqXHR); });
        } catch (exception) {
            zAlert(zExceptionMessage("@CSHTML", "function", exception.message));
        }
    });
</script>